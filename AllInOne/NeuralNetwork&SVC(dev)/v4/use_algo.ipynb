    "# Import the modules\n",
    "from sklearn.externals import joblib\n",
    "import numpy as np\n",
    "\n",
    "def predict(X):\n",
    "    # Load the classifier\n",
    "    clf = joblib.load(\"digits_cls.pkl\")\n",
    "\n",
    "    # Predict love.\n",
    "    nbr = clf.predict(np.array([X], 'float64'))\n",
    "    return nbr"
   ]
